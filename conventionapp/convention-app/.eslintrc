{
    "root": true,
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"  ],
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "prettier"  ],
    "rules": {
      "vue/multi-word-component-names": "off",
      "prettier/prettier": [
        "warn"
      ]
    }
}